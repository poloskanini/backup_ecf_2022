{% extends 'base.html.twig' %}

{% block title %}Partenaire{% endblock %}

{% block content %}

<div class="container">
    <h1>Partenaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ partenaire.name }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ partenaire.password }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ partenaire.email }}</td>
            </tr>
            <tr>
                <th>Planning</th>
                <td>{{ partenaire.planning ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Newsletter</th>
                <td>{{ partenaire.newsletter ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Boissons</th>
                <td>{{ partenaire.boissons ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Sms</th>
                <td>{{ partenaire.sms ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Promotions</th>
                <td>{{ partenaire.promotions ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Concours</th>
                <td>{{ partenaire.concours ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_partenaire_index') }}">back to list</a>
    <a href="{{ path('app_partenaire_edit', {'id': partenaire.id}) }}">edit</a>
</div>
    



